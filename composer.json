{
    "name": "jbrada/module-admin-order-product-links",
    "type": "magento2-module",
    "description": "Module enables link from admin items list to product edit page.",
    "license":"MIT",
    "authors": [
        {
            "name": "Jiří Brada",
            "email": "jiri@jbrada.cz",
            "homepage": "https://jbrada.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "magento/framework": "^103.0",
        "magento/module-sales": "*",
        "magento/module-catalog": "*"
    },
    "require-dev": {
	"magento/magento-coding-standard": "15",
        "phpstan/phpstan": "^1.2",
        "bitexpert/phpstan-magento": "^0.11.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "JBrada\\AdminOrderProductLinks\\": ""
        },
        "files": [
            "registration.php"
        ]
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo-magento-mirror.fooman.co.nz"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --ignore=vendor/ --standard=vendor/magento/magento-coding-standard/Magento2 ./",
        "phpcbf": "vendor/bin/phpcs --ignore=vendor/ --standard=vendor/magento/magento-coding-standard/Magento2 ./",
        "phpstan": "vendor/bin/phpstan analyze"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
